{
  "name": "ss-backend",
  "version": "1.00.17",
  "private": true,
  "scripts": {
    "dev": "ts-node index.ts",
    "build": "tsc && cp -r db/migrations dist/db/",
    "start": "node dist/db/migrate.js && node dist/index.js",
    "seed": "ts-node db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:push-schema": "ts-node db/push-schema.ts",
    "migrate": "ts-node db/migrate.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "clean-n-build": "rm -rf dist && rm -rf node_modules && pnpm install && pnpm run build",
    "setup": "pnpm install && pnpm run migrate && pnpm run seed && pnpm run build",
    "init-backend": "pnpm run setup && pnpm run dev"
  },
